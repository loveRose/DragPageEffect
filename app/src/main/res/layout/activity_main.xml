<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.app.lvyerose.drag.dragpageeffect.MainActivity">

    <com.app.lvyerose.drag.dragpageeffect.view.DragMultiLayout
        android:id="@+id/draw_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <FrameLayout
            android:id="@+id/head_frame_layout"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <FrameLayout
            android:id="@+id/between_frame_layout"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <FrameLayout
            android:id="@+id/bottom_frame_layout"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </com.app.lvyerose.drag.dragpageeffect.view.DragMultiLayout>

</RelativeLayout>
